{
  "$schema": "vscode://schemas/color-theme",
  "name": "Vim Default (6-Color Brown)",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#000000",
    "editorCursor.foreground": "#000000",
    "editor.selectionBackground": "#c0ddff44",
    "editorLineNumber.foreground": "#888888",
    "editorLineNumber.activeForeground": "#000000"
  },
  "tokenColors": [
    { "scope": ["comment"], "settings": { "foreground": "#0000FF", "fontStyle": "italic" } },
    { "scope": ["keyword","storage.type","storage.modifier"], "settings": { "foreground": "#804000", "fontStyle": "bold" } },
    { "scope": ["string","string.quoted","string.template"], "settings": { "foreground": "#008000" } },
    { "scope": ["constant","constant.numeric","constant.character","constant.other"], "settings": { "foreground": "#FF00FF" } },
    { "scope": ["entity.name.function","support.function"], "settings": { "fontStyle": "bold" } },
    { "scope": ["variable","identifier"], "settings": { "foreground": "#000000" } },
    { "scope": ["invalid","invalid.deprecated"], "settings": { "foreground": "#FFFFFF", "background": "#FF0000" } },
    { "scope": ["diff.inserted"], "settings": { "foreground": "#008000" } },
    { "scope": ["diff.deleted"], "settings": { "foreground": "#FF0000" } },
    { "scope": ["diff.changed"], "settings": { "foreground": "#0000FF" } }
  ]
}
